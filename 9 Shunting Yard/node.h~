//blueprint for node
#ifndef NODE_H
#define NODE_H

#include <iostream>
#include <cstring>

using namespace std;

class Node{
  Node* left;
  Node* right;
  char data;
 public:
  //constructor
  Node();
  Node(char newData);
  ~Node();
  
  //getters
  Node* getRight();
  Node* getLeft();
  char* getData();
  
  //setters
  void setLeft(Node* newLeft);
  void setRight(Node* newRight);
  void setData(char* newValue);
};

#endif
